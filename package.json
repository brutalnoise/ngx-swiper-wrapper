{
  "name": "ngx-swiper-wrapper",
  "description": "Angular wrapper library for Swiper",
  "bugs": "https://github.com/zefoy/ngx-swiper-wrapper/issues",
  "version": "10.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve app",
    "build": "ng build lib",
    "deploy": "deploy-to-git",
    "prepare": "ng build lib --prod",
    "publish": "npm publish ./dist/lib",
    "predeploy": "rimraf ./dist/app && mkdirp ./dist/app"
  },
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zefoy/ngx-swiper-wrapper.git"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/ngx-swiper-wrapper.git",
      "branch": "gh-pages",
      "folder": "dist/app",
      "script": "ng build app --prod -- --base-href=ngx-swiper-wrapper --delete-output-path=false",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^13.0.2",
    "@angular/common": "^13.0.2",
    "@angular/compiler": "^13.0.2",
    "@angular/core": "^13.0.2",
    "@angular/flex-layout": "12.0.0-beta.35",
    "@angular/platform-browser": "^13.0.2",
    "@angular/platform-browser-dynamic": "^13.0.2",
    "swiper": "^7.3.1"
  },
  "devDependencies": {
    "@angular/cli": "^13.0.3",
    "@angular/compiler-cli": "^13.0.2",
    "@angular-devkit/build-angular": "^13.0.3",
    "@types/node": "^16.11.10",
    "@types/swiper": "^5.4.3",
    "codelyzer": "^6.0.2",
    "core-js": "^3.19.1",
    "cpx": "^1.5.0",
    "deploy-to-git": "^0.1.5",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^13.0.8",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "terser": "^5.10.0",
    "ts-node": "^10.4.0",
    "tsickle": "^0.43.0",
    "tslint": "^6.0.0",
    "typescript": "4.4.4",
    "watch": "^1.0.2",
    "zone.js": "^0.11.4"
  }
}
